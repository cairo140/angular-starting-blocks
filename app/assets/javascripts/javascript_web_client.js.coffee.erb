@angular.JavascriptWebClient = @angular.module('angular.JavascriptWebClient', ['ngRoute'])
  .config ['$routeProvider', ($routeProvider) ->
    $routeProvider
      .when('/', controller: 'HomeCtrl', templateUrl: <%= asset_path('home.html').to_json %>)
      .when('/about', templateUrl: <%= asset_path('about.html').to_json %>)
      .otherwise(redirectTo: '/')
  ]
